@prefix spin: <http://spinrdf.org/sp/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rsine: <http://lod2.eu/rsine/> .
@prefix dcterms: <http://purl.org/dc/terms/>.

<http://example.org/aligned/new_scheme> a rsine:Subscription;
    rsine:query [
        dcterms:description "Notification custom schema creation";

        spin:text "PREFIX cs:<http://purl.org/vocab/changeset/schema#>
            PREFIX spin:<http://spinrdf.org/sp/>
            PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
            PREFIX skos:<http://www.w3.org/2004/02/skos/core#>
            PREFIX void:<http://rdfs.org/ns/void#>
            PREFIX dct:<http://purl.org/dc/terms/>
            PREFIX swcs:<http://schema.semantic-web.at/ppt/>
            PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>

            SELECT ?className WHERE {
                ?cs a cs:ChangeSet;
                    cs:createdDate ?csdate;
                    cs:addition ?classAdd;
                    cs:addition ?classInfo.

                ?classAdd rdf:subject ?class;
                    rdf:predicate rdf:type;
                    rdf:object rdfs:Class.

                ?schemeInfo rdf:subject ?class;
                    rdf:predicate rdfs:label;
                    rdf:object ?className.

                FILTER (?csdate > 'QUERY_LAST_ISSUED'^^<http://www.w3.org/2001/XMLSchema#dateTime>)
            }";

        rsine:formatter [
            a rsine:vtlFormatter;
            rsine:message "A custom class named '$bindingSet.getValue('className').getLabel()' has been added to scheme <unsupported> by user <unsupported>.";
        ]
    ];

    rsine:notifier [
        a rsine:loggingNotifier;
    ].