@prefix spin: <http://spinrdf.org/sp/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rsine: <http://lod2.eu/rsine/> .
@prefix dcterms: <http://purl.org/dc/terms/>.

<http://example.org/aligned/delete_scheme> a rsine:Subscription;
    rsine:query [
        dcterms:description "Notification custom schema deletion";

        spin:text "PREFIX cs:<http://purl.org/vocab/changeset/schema#>
            PREFIX spin:<http://spinrdf.org/sp/>
            PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
            PREFIX skos:<http://www.w3.org/2004/02/skos/core#>
            PREFIX void:<http://rdfs.org/ns/void#>
            PREFIX dct:<http://purl.org/dc/terms/>
            PREFIX swcs:<http://schema.semantic-web.at/ppt/>

            SELECT ?schemeName WHERE {
                ?cs a cs:ChangeSet;
                    cs:createdDate ?csdate;
                    cs:removal ?schemeRem;
                    cs:removal ?schemeIdentifier.

                ?schemeRem rdf:subject ?scheme;
                    rdf:predicate rdf:type;
                    rdf:object swcs:CustomSchema.

                ?schemeIdentifier rdf:subject ?scheme;
                    rdf:predicate swcs:identifier;
                    rdf:object ?schemeName.

                FILTER (?csdate > 'QUERY_LAST_ISSUED'^^<http://www.w3.org/2001/XMLSchema#dateTime>)
            }";

        rsine:formatter [
            a rsine:vtlFormatter;
            rsine:message "A custom scheme named '$bindingSet.getValue('schemeName').getLabel()' has been removed by user <unsupported>.";
        ]
    ];

    rsine:notifier [
        a rsine:loggingNotifier;
    ].